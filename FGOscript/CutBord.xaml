<Window x:Class="FGOscript.CutBord"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FGOscript"
        mc:Ignorable="d"
        Title="CutBord" Height="450" Width="800"
        WindowStyle="None"
        AllowsTransparency="True"
        WindowState="Maximized"
        Background="#02FFFFFF"
        Closing="Window_Closing"
        Loaded="Window_Loaded"
        >
    <Grid>
        <Rectangle Loaded="Rectangle_Loaded" Name="rectangle" Stroke="#FF47D3D3" HorizontalAlignment="Left" VerticalAlignment="Top" StrokeThickness="3"></Rectangle>
    </Grid>
</Window>
